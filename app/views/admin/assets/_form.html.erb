<form action="/admin/assets" method="post" enctype="multipart/form-data">

	<label for="asset_file">File</label>
	<input type="file" name="asset[file]" id="asset_file" />

	<input type="hidden" name="asset[path]" value="<%= @path %>" />
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>" />
	<div class="buttons">
	<input type="submit" value="Create" class="button submit" /> | <a href="#cancel" class="close">Cancel</a>
	</div>

</form>
